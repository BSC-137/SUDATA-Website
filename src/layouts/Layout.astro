---
import '../styles/global.css';

interface Props {
	title?: string;
}

const { title = "SUDATA" } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@400;700&family=VT323&display=swap" rel="stylesheet">
		<title>{title}</title>
	</head>
	<body class="min-h-screen flex flex-col bg-sudata-navy relative overflow-x-hidden selection:bg-sudata-cyan selection:text-sudata-navy">
		<!-- Retro Scanline Overlay -->
		<div class="fixed inset-0 pointer-events-none z-[60] bg-scanlines opacity-10"></div>
		<!-- Film Grain Overlay -->
		<div class="fixed inset-0 pointer-events-none z-[59] opacity-20 bg-noise mix-blend-overlay"></div>
		
		<!-- Glassmorphic Navbar -->
		<nav class="fixed top-6 left-0 right-0 mx-auto max-w-2xl z-50 flex justify-center">

			<!-- Expanded bar -->
			<div id="nav-expanded" style="transition: opacity 0.4s ease, transform 0.4s ease;" class="backdrop-blur-xl bg-white/5 border border-white/20 rounded-full shadow-2xl shadow-sudata-cyan/20">
				<div class="px-8 py-4">
					<div class="flex items-center justify-between gap-12">
						<!-- SUDATA: clicking collapses nav to pill (no page nav) -->
						<button id="sudata-logo" class="text-white font-bold text-lg tracking-wider hover:text-sudata-cyan transition-colors cursor-pointer bg-transparent border-none p-0 leading-none">
							SUDATA
						</button>
						<div class="flex space-x-2">
							<a href="/" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)]">Home</a>
							<a href="/about" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)]">About</a>
							<a href="/events" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)]">Events</a>
							<a href="/sponsors" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)]">Sponsors</a>
							<a href="/photos" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)]">Photos</a>
							<a href="https://usu.edu.au/clubs/sydney-uni-data-society/" target="_blank" rel="noopener noreferrer" class="text-sudata-cyan bg-sudata-cyan/10 border border-sudata-cyan/40 hover:bg-sudata-cyan/25 hover:border-sudata-cyan/70 hover:text-white hover:shadow-[0_0_20px_rgba(0,255,255,0.35)] hover:-translate-y-px px-4 py-2 rounded-full text-sm font-bold transition-all duration-300">Membership</a>
						</div>
					</div>
				</div>
			</div>

			<!-- Collapsed pill — hidden by default, clicking expands menu back -->
			<button id="nav-collapsed" style="position:absolute; top:0; left:50%; transform:translateX(-50%) scale(0.9); opacity:0; pointer-events:none; transition: opacity 0.4s ease, transform 0.4s ease;" class="backdrop-blur-xl bg-white/5 border border-white/20 rounded-full shadow-2xl shadow-sudata-cyan/20 px-8 py-4 text-white font-bold text-lg tracking-wider hover:text-sudata-cyan hover:border-sudata-cyan/40 cursor-pointer whitespace-nowrap">
				SUDATA
			</button>
		</nav>

		<main class="flex-grow max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-24 w-full relative z-10">
			<slot />
		</main>

		<footer class="border-t border-white/10 mt-auto relative z-10">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
				<div class="flex flex-col md:flex-row items-center justify-between gap-4">
					<div class="font-mono-tech text-sudata-grey/60 text-sm">
						© 2025 SUDATA — Sydney University Data Society
					</div>
					<div class="flex items-center gap-6">
						<!-- Instagram -->
						<a href="https://www.instagram.com/usyd.sudata/" target="_blank" rel="noopener noreferrer" class="text-sudata-grey/60 hover:text-sudata-cyan transition-colors duration-200" aria-label="Instagram">
							<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="2" y="2" width="20" height="20" rx="5" ry="5"/><path d="M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z"/><line x1="17.5" y1="6.5" x2="17.51" y2="6.5"/></svg>
						</a>
						<!-- LinkedIn -->
						<a href="https://www.linkedin.com/company/sudata/" target="_blank" rel="noopener noreferrer" class="text-sudata-grey/60 hover:text-sudata-cyan transition-colors duration-200" aria-label="LinkedIn">
							<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M16 8a6 6 0 0 1 6 6v7h-4v-7a2 2 0 0 0-2-2 2 2 0 0 0-2 2v7h-4v-7a6 6 0 0 1 6-6z"/><rect x="2" y="9" width="4" height="12"/><circle cx="4" cy="4" r="2"/></svg>
						</a>
						<!-- XHS (RedNote) -->
						<a href="#" class="text-sudata-grey/60 hover:text-sudata-cyan transition-colors duration-200" aria-label="RedNote">
							<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="currentColor">
								<defs>
									<filter id="xhs-grey" color-interpolation-filters="sRGB">
										<feColorMatrix type="saturate" values="0"/>
										<feComponentTransfer>
											<feFuncR type="linear" slope="0.6"/>
											<feFuncG type="linear" slope="0.6"/>
											<feFuncB type="linear" slope="0.6"/>
										</feComponentTransfer>
									</filter>
								</defs>
								<image href="data:image/png;base64," width="20" height="20" filter="url(#xhs-grey)"/>
							</svg>
						</a>
						<!-- USU Membership signup -->
						<a href="https://usu.edu.au/clubs/sydney-uni-data-society/" target="_blank" rel="noopener noreferrer" class="text-sudata-grey/60 hover:text-sudata-cyan transition-colors duration-200" aria-label="USU Membership">
							<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"/><circle cx="12" cy="7" r="4"/></svg>
						</a>
					</div>
				</div>
			</div>
		</footer>
		<script>
			// Advanced Vapor Pixel Cursor Trail
			document.addEventListener('mousemove', (e) => {
				const pixel = document.createElement('div');
				pixel.classList.add('pixel-trail');
				
				// Snap to grid for pixel effect (e.g., 4px grid)
				const gridSize = 4;
				const x = Math.floor(e.clientX / gridSize) * gridSize;
				const y = Math.floor(e.clientY / gridSize) * gridSize;
				
				// Randomize life and size slightly for "vapor" look
				const randomScale = 0.5 + Math.random(); 
				const randomLife = 400 + Math.random() * 400; 
				
				pixel.style.left = `${x}px`;
				pixel.style.top = `${y}px`;
				pixel.style.transform = `scale(${randomScale})`;
				pixel.style.opacity = `${0.5 + Math.random() * 0.5}`;
				
				document.body.appendChild(pixel);
				
				setTimeout(() => {
					pixel.remove();
				}, randomLife);
			});

			// Scroll-Driven "Data Stream" Reveal
			const observerOptions = {
				root: null,
				rootMargin: '0px',
				threshold: 0.15 
			};

			const observer = new IntersectionObserver((entries, observer) => {
				entries.forEach(entry => {
					if (entry.isIntersecting) {
                        // Enter viewport: Fade In & Slide Up
						entry.target.classList.remove('opacity-0', 'translate-y-[40px]');
						entry.target.classList.add('opacity-100', 'translate-y-0');
					} else {
                        // Leave viewport: Fade Out & Slide Down
                        entry.target.classList.remove('opacity-100', 'translate-y-0');
						entry.target.classList.add('opacity-0', 'translate-y-[40px]');
                    }
				});
			}, observerOptions);

			document.querySelectorAll('.reveal-on-scroll').forEach((el) => {
                // Initial State
				el.classList.add('opacity-0', 'translate-y-[40px]', 'transition-all', 'duration-1000', 'ease-[cubic-bezier(0.16,1,0.3,1)]');
				observer.observe(el);
			});

			// ── Collapsible Navbar (click SUDATA to collapse/expand) ──────────
			const navExpanded = document.getElementById('nav-expanded');
			const navCollapsed = document.getElementById('nav-collapsed');
			const sudataLogo = document.getElementById('sudata-logo');

			let isCollapsed = false;

			function collapseNav() {
				isCollapsed = true;
				navExpanded.style.opacity = '0';
				navExpanded.style.transform = 'scale(0.9)';
				navExpanded.style.pointerEvents = 'none';
				navCollapsed.style.opacity = '1';
				navCollapsed.style.transform = 'translateX(-50%) scale(1)';
				navCollapsed.style.pointerEvents = 'auto';
			}

			function expandNav() {
				isCollapsed = false;
				navExpanded.style.opacity = '1';
				navExpanded.style.transform = 'scale(1)';
				navExpanded.style.pointerEvents = 'auto';
				navCollapsed.style.opacity = '0';
				navCollapsed.style.transform = 'translateX(-50%) scale(0.9)';
				navCollapsed.style.pointerEvents = 'none';
			}

			// SUDATA in expanded bar: toggle collapse (no navigation)
			sudataLogo.addEventListener('click', () => {
				if (!isCollapsed) collapseNav();
			});

			// Collapsed pill: expand back
			navCollapsed.addEventListener('click', () => {
				if (isCollapsed) expandNav();
			});
		</script>
	</body>
</html>
