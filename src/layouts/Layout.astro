---
import '../styles/global.css';
import SubcomRecruitment from '../components/SubcomRecruitment.jsx';

interface Props {
	title?: string;
}

const { title = "SUDATA" } = Astro.props;
const isHomePage = Astro.url.pathname === '/';
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0" />
		<link rel="icon" type="image/svg+xml" href="/sudino.svg" />
		<meta name="generator" content={Astro.generator} />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@400;700&family=VT323&display=swap" rel="stylesheet">
		<title>{title}</title>
	</head>
	<body class="min-h-screen flex flex-col bg-sudata-navy relative overflow-x-hidden selection:bg-sudata-cyan selection:text-sudata-navy">
		<!-- Retro Scanline Overlay -->
		<div class="fixed inset-0 pointer-events-none z-[60] bg-scanlines opacity-10"></div>
		<!-- Film Grain Overlay -->
		<div class="fixed inset-0 pointer-events-none z-[59] opacity-20 bg-noise mix-blend-overlay"></div>
		
		<!-- Glassmorphic Navbar -->
		<nav id="main-nav" class="fixed top-6 left-0 right-0 mx-auto max-w-fit z-50 flex justify-center transition-all duration-500">
			<div class="backdrop-blur-xl bg-white/5 border border-white/20 rounded-full shadow-2xl shadow-sudata-cyan/20">
				<div id="nav-pill-inner" class="px-8 py-4">
					<div id="nav-inner" class="flex items-center gap-12 transition-all duration-500">
						<!-- Logo/Brand -->
						<button id="nav-toggle" class="relative text-white font-bold text-lg tracking-wider hover:text-sudata-cyan transition-colors cursor-pointer bg-transparent border-none p-0 focus:outline-none whitespace-nowrap">
							<img
								id="sudino-icon"
								src="/sudino.svg"
								alt="Sudino"
								class="absolute -top-5 right-0 w-7 h-7 pointer-events-none transition-all duration-500"
								style="opacity:0; transform: scale(0.5);"
							/>
							SU<span>DATA</span>
						</button>
						
						<!-- Navigation Links -->
						<div id="nav-links" class="flex space-x-2 transition-all duration-500" style="opacity: 1;">
							<a href="/" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)] whitespace-nowrap">
								Home
							</a>
							<a href="/about" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)] whitespace-nowrap">
								About
							</a>
							<a href="/events" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)] whitespace-nowrap">
								Events
							</a>
							<a href="/sponsors" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)] whitespace-nowrap">
								Sponsors
							</a>
							<a href="/careers" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)] whitespace-nowrap">
								Careers
							</a>
							<a href="/photos" class="text-white/80 hover:text-white px-4 py-2 rounded-full text-sm font-medium transition-all duration-300 hover:bg-white/10 hover:shadow-[0_0_10px_rgba(255,255,255,0.2)] whitespace-nowrap">
								Photos
							</a>
							<a href="https://usu.edu.au/clubs/sydney-uni-data-society/" target="_blank" rel="noopener noreferrer" class="text-sudata-cyan bg-sudata-cyan/10 border border-sudata-cyan/40 hover:bg-sudata-cyan/25 hover:border-sudata-cyan/70 hover:text-white hover:shadow-[0_0_20px_rgba(0,255,255,0.35)] hover:-translate-y-px px-4 py-2 rounded-full text-sm font-bold transition-all duration-300 whitespace-nowrap">
								Membership
							</a>
						</div>
					</div>
				</div>
			</div>
		</nav>
		<style>
			/* Remove tap/touch highlight on nav links so iOS doesn't leave a
			   persistent white ring on the last-tapped link */
			#nav-links a {
				-webkit-tap-highlight-color: transparent;
				outline: none;
			}

			@media (max-width: 639px) {
				#main-nav.nav-mobile-expanded {
					max-width: calc(100% - 2rem) !important;
					border-radius: 1rem;
				}
				/* Tighten the outer pill padding so the expanded menu is compact */
				#main-nav.nav-mobile-expanded #nav-pill-inner {
					padding: 0.5rem 0.75rem;
				}
				#main-nav.nav-mobile-expanded #nav-links {
					flex-wrap: wrap;
					justify-content: center;
					gap: 0.2rem;
				}
				#main-nav.nav-mobile-expanded #nav-links a {
					padding: 0.3rem 0.55rem;
					font-size: 0.7rem;
					/* Centre text inside each button (including Membership) */
					text-align: center;
					display: inline-flex;
					align-items: center;
					justify-content: center;
				}
			}
		</style>
		<script>
			(function() {
				const toggle = document.getElementById('nav-toggle');
				const navLinks = document.getElementById('nav-links');
				const navInner = document.getElementById('nav-inner');
				const sudinoIcon = document.getElementById('sudino-icon');
				const mainNav = document.getElementById('main-nav');
				// Check if mobile view
				const isMobile = () => window.innerWidth < 640;

				// Initialise collapsed state immediately to prevent FOUC on mobile
				let collapsed = isMobile();
				if (collapsed) {
					navLinks.style.opacity = '0';
					navLinks.style.visibility = 'hidden';
					navLinks.style.width = '0';
					navLinks.style.overflow = 'hidden';
					navInner.style.gap = '0';
					sudinoIcon.style.opacity = '1';
					sudinoIcon.style.transform = 'scale(1)';
					mainNav.classList.remove('nav-mobile-expanded');
				}

				// Handle toggle click
				toggle.addEventListener('click', () => {
					collapsed = !collapsed;
					if (collapsed) {
						navLinks.style.opacity = '0';
						navLinks.style.visibility = 'hidden';
						navLinks.style.width = '0';
						navLinks.style.overflow = 'hidden';
						navInner.style.gap = '0';
						sudinoIcon.style.opacity = '1';
						sudinoIcon.style.transform = 'scale(1)';
						mainNav.classList.remove('nav-mobile-expanded');
					} else {
						navLinks.style.opacity = '1';
						navLinks.style.visibility = 'visible';
						navLinks.style.width = '';
						navLinks.style.overflow = '';
						navInner.style.gap = '';
						sudinoIcon.style.opacity = '0';
						sudinoIcon.style.transform = 'scale(0.5)';
						if (isMobile()) mainNav.classList.add('nav-mobile-expanded');
					}
				});

				// Handle window resize
				window.addEventListener('resize', () => {
					if (!isMobile()) {
						navLinks.style.opacity = '1';
						navLinks.style.visibility = 'visible';
						navLinks.style.width = '';
						navLinks.style.overflow = '';
						navInner.style.gap = '';
						collapsed = false;
						mainNav.classList.remove('nav-mobile-expanded');
					}
				});
			})();
		</script>

		<main class="flex-grow max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-20 sm:py-24 w-full relative z-10">
			<slot />
		</main>

		<footer class="border-t border-sudata-grey/20 py-4 sm:py-6 mt-auto relative z-10 bg-sudata-navy/80 backdrop-blur-sm">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 flex flex-col sm:flex-row items-center justify-between gap-3 sm:gap-4">
				<div class="flex flex-col items-center sm:items-start gap-1">
					<p class="text-sm text-sudata-grey/60">&copy; {new Date().getFullYear()} SUDATA. All rights reserved.</p>
					<div class="flex items-center gap-1.5 flex-wrap justify-center sm:justify-start">
						<span class="text-[11px] text-sudata-grey/35 font-mono">Technical issues? Reach us at</span>
						<span class="inline-flex items-center gap-1">
							<span class="text-[11px] text-sudata-grey/35 font-mono">technology@sudata.org</span>
							<!-- Copy email button -->
							<button
								id="copy-tech-email"
								class="text-sudata-grey/35 hover:text-sudata-grey/60 transition-colors duration-200 p-0.5 rounded"
								aria-label="Copy email address"
								title="Copy email address"
							>
								<svg xmlns="http://www.w3.org/2000/svg" width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
									<path d="M8 16H6a2 2 0 01-2-2V6a2 2 0 012-2h8a2 2 0 012 2v2m-6 12h8a2 2 0 002-2v-8a2 2 0 00-2-2h-8a2 2 0 00-2 2v8a2 2 0 002 2z"/>
								</svg>
							</button>
							<!-- Draft email button -->
							<a
								href="mailto:technology@sudata.org?subject=Technical%20Issue%20%E2%80%94%20SUDATA%20Website&body=Hi%20SUDATA%20Tech%20Team%2C%0A%0AI%27m%20writing%20to%20flag%20a%20technical%20issue%20with%20the%20SUDATA%20website.%0A%0A%5BDescribe%20the%20issue%20here%5D%0A%0AThanks%2C%0A%5BYour%20name%5D"
								class="text-sudata-grey/35 hover:text-sudata-grey/60 transition-colors duration-200 p-0.5 rounded"
								aria-label="Draft email to tech team"
								title="Draft email to tech team"
							>
								<svg xmlns="http://www.w3.org/2000/svg" width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
									<path d="M17 3a2.85 2.83 0 1 1 4 4L7.5 20.5 2 22l1.5-5.5Z"/>
								</svg>
							</a>
						</span>
					</div>
				</div>
				<div class="flex flex-wrap items-center gap-4 gap-y-2">
					<!-- Email -->
					<a href="mailto:sydney.uni.data.society@gmail.com" class="text-sudata-grey/60 hover:text-sudata-cyan transition-colors duration-200" aria-label="Email us">
						<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="2" y="4" width="20" height="16" rx="2"/><path d="m22 7-8.97 5.7a1.94 1.94 0 0 1-2.06 0L2 7"/></svg>
